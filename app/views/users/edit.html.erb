<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>
<%# 
<%= form_for(resource, as: resource_name, url: edit_user_path(resource_name), html: { method: :put }) do |f| %>
  <%# <%= render "vdevise/shared/error_messages", resource: resource %> 
  <%= form_with(model: @user, local: true) do |f| %>
  
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
  </div>
<%= f.submit 'プロフィール編集を完了する', class: 'btn btn-primary' %></div>


<% end %>